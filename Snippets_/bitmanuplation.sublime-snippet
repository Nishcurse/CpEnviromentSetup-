// Bit Manupulation
ll setBit_count(ll n){
    return  __builtin_popcountll(n); 
}
ll highesSet_bit(ll n){
    return (63 - __builtin_clzll(n)); 
}
ll trailZeros(ll n){
	return __builtin_ctzll(n); 
}

]]></content>
